{
  "name": "tinkerun-vscode",
  "version": "0.0.0",
  "author": "billyct <billy_allen@126.com>",
  "license": "MIT",
  "main": "./build/extension.js",
  "engines": {
    "vscode": "^1.55.0"
  },
  "activationEvents": [
    "onCommand:tinkerun.install",
    "onCommand:tinkerun.run"
  ],
  "contributes": {
    "commands": [
      {
        "command": "tinkerun.install",
        "title": "Tinkerun: Install"
      },
      {
        "command": "tinkerun.run",
        "title": "Tinkerun: Run",
        "icon": "$(debug-start)"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "tinkerun.run",
          "when": "resourceDirname =~ /\\.tinkerun/ && resourceLangId == php",
          "group": "navigation"
        }
      ],
      "editor/title/run": [
        {
          "command": "tinkerun.run",
          "when": "resourceDirname =~ /\\.tinkerun/ && resourceLangId == php"
        }
      ]
    }
  },
  "scripts": {
    "start": "node ./build.js",
    "build": "NODE_ENV=production node ./build.js",
    "lint": "ts-standard \"./src/**/*.ts\""
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.37",
    "@types/vscode": "^1.55.0",
    "esbuild": "^0.11.10",
    "ts-standard": "^10.0.0",
    "typescript": "^4.2.4",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
